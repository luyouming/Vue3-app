{
  "name": "shopping-mall",
  "version": "1.2.0",
  "scripts": {
    "start": "vite --host",
    "dev": "vite --host",
    "watch": "nodemon --watch vite.config.js --exec \" vite --host \" ",
    "serve": "vite preview --host",
    "build:dev": "vite build --mode dev",
    "build": "vite build",
    "zip": "npm run build && node ./zip.js",
    "git": "tive git -c tive.git.config.js",
    "lint": "eslint \"src/**/*\" --fix",
    "prettier": "prettier --write ."
  },
  "dependencies": {
    "axios": "^0.21.1",
    "vant": "^3.0.7",
    "vue": "^3.2.24",
    "vue-router": "^4.0.4",
    "vuex": "^4.0.0",
    "vue3-lazy": "^1.0.0-alpha.1",
    "vue3-marquee": "^2.0.2-beta",
    "tcfe-helper": "^1.1.3"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.10.1",
    "@typescript-eslint/parser": "^5.10.1",
    "@vitejs/plugin-vue": "^1.1.4",
    "@vue/compiler-sfc": "^3.2.24",
    "adm-zip": "^0.5.9",
    "autoprefixer": "^10.2.4",
    "eslint": "^8.7.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^8.3.0",
    "lib-flexible": "^0.3.2",
    "postcss-loader": "^4.1.0",
    "postcss-px2rem": "^0.3.0",
    "postcss-px2rem-exclude": "^0.0.6",
    "prettier": "^2.5.1",
    "rimraf": "^3.0.2",
    "sass": "^1.32.8",
    "vite": "^2.0.0"
  },
  "autoprefixer": {
    "browsers": [
      "> 1%",
      "last 2 versions",
      "not ie <= 8",
      "Android >= 4.0",
      "iOS >= 8"
    ]
  }
}
